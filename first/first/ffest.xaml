<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="first.ffest"
             xmlns:pages="clr-namespace:first"
 NavigationPage.HasNavigationBar="False">
  <FlyoutPage.Flyout>
    <pages:ffestFlyout x:Name="FlyoutPage" />
  </FlyoutPage.Flyout>
  <FlyoutPage.Detail>
    <NavigationPage>
      <x:Arguments>
        <pages:ffestDetail />
      </x:Arguments>
    </NavigationPage>
  </FlyoutPage.Detail>
    
    
</FlyoutPage>